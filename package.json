{
  "name": "devpath-app",
  "version": "1.0.0",
  "description": "Devpath MERN App application to manage your career path in IT",
  "main": "server.js",
  "scripts": {
    "start": "SET NODE_ENV=production & node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexxtGen/devpath-app.git"
  },
  "keywords": [
    "MERN",
    "IT",
    "career",
    "fullstack",
    "app"
  ],
  "author": "Kamil S",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nexxtGen/devpath-app/issues"
  },
  "homepage": "https://github.com/nexxtGen/devpath-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gh-polyglot": "^2.3.2",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.10",
    "morgan": "^1.9.1",
    "node-geocoder": "^3.25.0"
  },
  "devDependencies": {
    "moment-locales-webpack-plugin": "^1.2.0",
    "nodemon": "^2.0.2"
  }
